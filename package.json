    "release": "npm run upgrade && npm run build && npm run bump && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eformat/swagger-node-petstore.git"
  },
  "dependencies": {
    "swagger-server": ">=1.0.0-alpha.16"
  },
  "devDependencies": {
    "jscs": "^2.1.1",
    "jshint": "^2.8.0",
    "npm-check-updates": "^2.2.2",
    "version-bump-prompt": "^1.5.1"
  }
}
