{
  "name": "swagger-node-petstore",
  "version": "1.0.0",
  "description": "REST API for Swagger Server",
  "keywords": [
    "express",
    "swagger",
    "server",
    "samples",
    "mock",
    "fake",
    "stub",
    "test",
    "rest",
    "api"
  ],
  "author": {
    "name": "Mike Hepburn",
    "url": "https://github.com/eformat/swagger-node-petstore"
  },
  "license": "MIT",
  "homepage": "https://github.com/eformat/swagger-node-petstore",
  "main": "README.md",
  "scripts": {
    "start": "node server.js",
    "build": "jshint . --verbose && jscs . --verbose",
